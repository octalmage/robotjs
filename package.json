{
  "name": "robotjs",
  "version": "0.5.1",
  "description": "Node.js Desktop Automation.",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "run-script-os",
    "test:darwin:linux": "jasmine 'test/**/*.js'",
    "test:win32": "jasmine test/**/*.js",
    "install": "prebuild-install || node-gyp rebuild",
    "build": "node-gyp --debug configure rebuild",
    "build:release": "node-gyp build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octalmage/robotjs.git"
  },
  "keywords": [
    "Automation",
    "GUI",
    "mouse",
    "keyboard",
    "screenshot",
    "image",
    "pixel",
    "desktop",
    "robotjs",
    "screen",
    "recognition",
    "autohotkey",
    "machine",
    "learning",
    "color"
  ],
  "author": "Jason Stallings",
  "license": "MIT",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/octalmage/robotjs/issues"
  },
  "homepage": "https://github.com/octalmage/robotjs",
  "dependencies": {
    "conventional-changelog": "^3.1.15",
    "node-addon-api": "*",
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "jasmine": "^3.5.0",
    "jest": "^24.9.0",
    "nan": "^2.14.0",
    "node-gyp": "^6.0.1",
    "prebuild": "^9.1.1",
    "run-script-os": "^1.0.7",
    "targetpractice": "0.0.7"
  },
  "jest": {
    "testMatch": [
      "**/test/**/*.[jt]s?(x)"
    ]
  }
}
